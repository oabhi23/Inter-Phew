<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abhi.appster">

    <application
        android:allowBackup="true"
        android:icon="@drawable/application"
        android:label="Inter-Phew"
        android:roundIcon="@drawable/application"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".JobEntry"
            android:label="@string/title_activity_job_entry"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ApplicationListView"></activity>
    </application>

</manifest>